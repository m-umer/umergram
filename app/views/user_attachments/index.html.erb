<p id="notice"><%= notice %></p>

<h1>Listing User Attachments</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_attachments.each do |user_attachment| %>
      <tr>
        <td><%= user_attachment.user_id %></td>
        <td><%= user_attachment.image %></td>
        <td><%= link_to 'Show', user_attachment, class: "btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_user_attachment_path(user_attachment), class: "btn btn-primary" %></td>
        <td><%= link_to 'Destroy', user_attachment, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User attachment', new_user_attachment_path, class: "btn btn-primary" %>
